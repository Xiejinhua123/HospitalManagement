<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name></display-name>
  <servlet>
    <servlet-name>PatientsServlet</servlet-name>
    <servlet-class>com.accp.servlet.PatientsServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DoctorServlet</servlet-name>
    <servlet-class>com.accp.servlet.DoctorServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DepartmentServlet</servlet-name>
    <servlet-class>com.accp.servlet.DepartmentServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RegiteredServlet</servlet-name>
    <servlet-class>com.accp.servlet.RegiteredServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DoctorOfferServlet</servlet-name>
    <servlet-class>com.accp.servlet.DoctorOfferServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SpecialistServlet</servlet-name>
    <servlet-class>com.accp.servlet.SpecialistServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PrescriptionServlet</servlet-name>
    <servlet-class>com.accp.servlet.PrescriptionServlet</servlet-class>
  </servlet>





  <servlet-mapping>
    <servlet-name>PatientsServlet</servlet-name>
    <url-pattern>/Patients</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DoctorServlet</servlet-name>
    <url-pattern>/doctor</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DepartmentServlet</servlet-name>
    <url-pattern>/Department</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegiteredServlet</servlet-name>
    <url-pattern>/registered</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DoctorOfferServlet</servlet-name>
    <url-pattern>/DoctorOffer</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SpecialistServlet</servlet-name>
    <url-pattern>/specialist</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PrescriptionServlet</servlet-name>
    <url-pattern>/Prescription</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>/Department</welcome-file>
  </welcome-file-list>
</web-app>